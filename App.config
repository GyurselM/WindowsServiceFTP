<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
	</configSections>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>
	
	<!--Log4net-->
	<log4net>
		<appender name="MainFileAppender" type="log4net.Appender.FileAppender">
			<file type="log4net.Util.PatternString" value="Logs/logfile_%date{yyyy-MM-dd_HH-mm}.log" />
			<encoding value="utf-8" />
			<appendToFile value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date > [%logger]{%method} > %level:: %message%n" />
			</layout>
		</appender>
		<root>
			<level value="ALL" />
			<appender-ref ref="MainFileAppender" />
		</root>
	</log4net>
	
	<!--Añadir appSettings-->
